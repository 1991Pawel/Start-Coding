{
	"$schema": "https://turborepo.org/schema.json",
	"globalDependencies": ["$PORT"],
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"env": ["NEXT_PUBLIC_GRAPH_API_URI"],
			"outputs": ["dist/**", ".next/**"]
		},
		"lint": {
			"outputs": []
		},
		"test": {
			"outputs": []
		},
		"dev": {
			"cache": false
		}
	}
}
